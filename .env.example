# .env.example - template for environment variables

# Database URL (SQLite for local dev, Postgres/MySQL for production)
DATABASE_URL=sqlite:///./data.db

# OddsAPI key placeholder - replace in local .env or cloud env vars
ODDS_API_KEY=your_api_key_here

# Mock feed for local testing (true/false)
USE_MOCK_FEED=true

# Mock feed interval in seconds
MOCK_FEED_INTERVAL=30
